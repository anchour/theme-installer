{
    "name": "anchour/theme-installer",
    "description": "Simple installer package for Anchour base theme.",
    "type": "metapackage",
    "version": "0.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Robitaille",
            "email": "matt@anchour.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anchour\\Theme\\Installer\\": "src/"
        }
    },
    "require": {
        "illuminate/console": "~5.6",
        "illuminate/filesystem": "~5.6",
        "symfony/process": "~3.3",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.3.1"
    },
    "scripts": {
        "test": [
            "phpcs src --extensions=php --standard=PSR2"
        ]
    }
}
